@model Blog
@{
	Layout = "_DashBoard";
}

<div class="overview">
	<div class="title">
		<i class="uil uil-tachometer-fast-alt"></i>
		<span class="text">Adding Blog</span>
	</div>

	<div>
		<form method="post" asp-action="AddBlog" class="row">
			<div class="mb-3">
				<label asp-for="Name" class="form-label">Product Name</label>
				<input type="text"
					   class="form-control"
					   placeholder=""
					   asp-for="Name" />

			</div>


			<div class="col-lg-12 d-flex justify-content-center  ">

				<button type="submit" class="btn btn-primary">Submit</button>

			</div>
		</form>

	</div>
</div>

